{
    "routes": [
      {
        "route": "/",
        "serve": "/index.html",
        "statusCode": 200
      },
      {
        "route": "/*",
        "serve": "/index.html",
        "statusCode": 200
      }
    ],
    "navigationFallback": {
      "rewrite": "/index.html",
      "exclude": ["/images/*", "/assets/*", "/favicon.ico", "/robots.txt"]
    },
    "responseOverrides": {
      "404": {
        "rewrite": "/index.html"
      }
    },
    "mimeTypes": {
      ".js": "application/javascript",
      ".css": "text/css",
      ".svg": "image/svg+xml"
    }
  }